{% extends "main_layout.html.twig" %}

{% set includeHeader = true %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('page') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('page') }}
{% endblock %}

{% block pageclass %}{% endblock %}

{% block content %}

    <div class="container page-content">
        <div class="card">
            <div class="card-body shadow-sm">
                {% if page is defined and page.page.content is defined %}
                {{ page.page.content|raw }}
                {% endif %}
            </div>
        </div>
    </div>

{% endblock %}

